<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zeus.vibin.conv">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".button.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".button.main.Convert" />
        <activity android:name=".button.main.Info" />
        <activity
            android:name=".NavDrawer"
            android:label="@string/title_activity_nav_drawer"
            android:parentActivityName=".button.main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.zeus.vibin.conv.button.main.MainActivity" />
        </activity>
        <activity android:name=".button.main.Ed"></activity>
    </application>

</manifest>